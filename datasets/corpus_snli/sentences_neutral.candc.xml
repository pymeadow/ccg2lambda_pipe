<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="ba" cat="NP">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="person" lemma="person" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="NP\NP">
    <lf start="2" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="horse" lemma="horse" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <rule type="fa" cat="S[dcl]\NP">
   <lf start="5" span="1" word="jumps" lemma="jump" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
   <rule type="fa" cat="PP">
    <lf start="6" span="1" word="over" lemma="over" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
    <rule type="ba" cat="NP">
     <lf start="7" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP" />
     <rule type="lex" cat="NP\NP">
      <rule type="ba" cat="S[pss]\NP">
       <lf start="8" span="1" word="broken" lemma="break" pos="VBN" chunk="I-NP" entity="O" cat="S[pss]\NP" />
       <rule type="ba" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="9" span="1" word="down" lemma="down" pos="RP" chunk="I-PRT" entity="O" cat="(S\NP)\(S\NP)" />
        <lf start="10" span="1" word="airplane." lemma="airplane." pos="NN" chunk="I-NP" entity="O" cat="((S\NP)\(S\NP))\((S\NP)\(S\NP))" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="person" lemma="person" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="fa" cat="S[dcl]\NP">
   <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
   <rule type="fa" cat="S[ng]\NP">
    <lf start="3" span="1" word="training" lemma="train" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="horse" lemma="horse" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="6" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="7" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="8" span="1" word="competition." lemma="competition." pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
