<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="tr" cat="S[X]/(S[X]/NP)">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="An" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="older" lemma="older" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="man" lemma="man" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="drinks" lemma="drink" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fc" cat="S[dcl]/NP">
    <rule type="tr" cat="S[X]/(S[X]\NP)">
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="his" lemma="his" pos="PRP$" chunk="B-NP" entity="O" cat="NP[nb]/N" />
       <lf start="5" span="1" word="juice" lemma="juice" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="6" span="1" word="as" lemma="as" pos="IN" chunk="I-SBAR" entity="O" cat="(NP\NP)/NP" />
       <lf start="7" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
      </rule>
     </rule>
    </rule>
    <rule type="bx" cat="(S[dcl]\NP)/NP">
     <lf start="8" span="1" word="waits" lemma="wait" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="9" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="10" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="11" span="1" word="daughter" lemma="daughter" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="lex" cat="NP\NP">
        <rule type="fa" cat="S[to]\NP">
         <lf start="12" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
         <rule type="fa" cat="S[b]\NP">
          <rule type="bx" cat="(S[b]\NP)/NP">
           <lf start="13" span="1" word="get" lemma="get" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
           <lf start="14" span="1" word="off" lemma="off" pos="RP" chunk="I-PRT" entity="O" cat="(S\NP)\(S\NP)" />
          </rule>
          <rule type="lex" cat="NP">
           <lf start="15" span="1" word="work" lemma="work" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="16" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="rtc" cat="S[X]/S[X]">
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="1" span="1" word="few" lemma="few" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="2" span="1" word="people" lemma="people" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="3" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="restaurant" lemma="restaurant" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="6" span="1" word="setting" lemma="setting" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="7" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   </rule>
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <lf start="8" span="1" word="one" lemma="one" pos="CD" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="9" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <lf start="10" span="1" word="them" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="11" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
     <rule type="fa" cat="S[ng]\NP">
      <lf start="12" span="1" word="drinking" lemma="drink" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="13" span="1" word="orange" lemma="orange" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="14" span="1" word="juice" lemma="juice" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="15" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="people" lemma="people" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="fa" cat="S[ng]\NP">
     <lf start="3" span="1" word="eating" lemma="eat" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="4" span="1" word="omelettes" lemma="omelette" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="people" lemma="people" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="ba" cat="S[ng]\NP">
     <rule type="ba" cat="S[ng]\NP">
      <lf start="3" span="1" word="sitting" lemma="sit" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="4" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="lex" cat="NP">
        <lf start="5" span="1" word="desks" lemma="desk" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="7" span="1" word="school" lemma="school" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="diners" lemma="diner" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="3" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="restaurant" lemma="restaurant" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
