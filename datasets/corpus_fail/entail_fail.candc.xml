<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="school" lemma="school" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="ba" cat="S[ng]\NP">
     <rule type="fa" cat="S[ng]\NP">
      <lf start="3" span="1" word="having" lemma="have" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="special" lemma="special" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="6" span="1" word="event" lemma="event" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <rule type="fa" cat="((S[X]\NP)\(S[X]\NP))/(S[to]\NP)">
       <lf start="7" span="1" word="in" lemma="in" pos="IN" chunk="I-SBAR" entity="O" cat="(((S\NP)\(S\NP))/(S[to]\NP))/N" />
       <lf start="8" span="1" word="order" lemma="order" pos="NN" chunk="O" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="S[to]\NP">
       <lf start="9" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
       <rule type="fa" cat="S[b]\NP">
        <lf start="10" span="1" word="show" lemma="show" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
        <rule type="ba" cat="NP">
         <rule type="fa" cat="NP[nb]">
          <lf start="11" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="12" span="1" word="american" lemma="american" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="13" span="1" word="culture" lemma="culture" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="fa" cat="NP\NP">
          <lf start="14" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/S[qem]" />
          <rule type="fa" cat="S[qem]">
           <rule type="fa" cat="S[qem]/(S[dcl]\NP)">
            <rule type="fa" cat="(S[qem]/(S[dcl]\NP))/N">
             <lf start="15" span="1" word="how" lemma="how" pos="WRB" chunk="I-ADVP" entity="O" cat="((S[qem]/(S[dcl]\NP))/N)/(S[adj]\NP)" />
             <lf start="16" span="1" word="other" lemma="other" pos="JJ" chunk="I-NP" entity="O" cat="S[adj]\NP" />
            </rule>
            <lf start="17" span="1" word="cultures" lemma="culture" pos="NNS" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="fa" cat="S[dcl]\NP">
            <lf start="18" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
            <rule type="ba" cat="S[pss]\NP">
             <rule type="fa" cat="S[pss]\NP">
              <lf start="19" span="1" word="dealt" lemma="deal" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/(PP/NP)" />
              <lf start="20" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
             </rule>
             <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
              <lf start="21" span="1" word="in" lemma="in" pos="IN" chunk="B-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
              <rule type="lex" cat="NP">
               <lf start="22" span="1" word="parties" lemma="party" pos="NNS" chunk="I-NP" entity="O" cat="N" />
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="23" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="group" lemma="group" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="3" span="1" word="boys" lemma="boy" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="4" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="fa" cat="S[ng]\NP">
     <lf start="5" span="1" word="celebrating" lemma="celebrate" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="6" span="1" word="Native" lemma="native" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="7" span="1" word="American" lemma="american" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="8" span="1" word="culture" lemma="culture" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
