<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Two" lemma="two" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="adults" lemma="adult" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="ba" cat="S[dcl]\NP">
     <lf start="2" span="1" word="run" lemma="run" pos="VBP" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="3" span="1" word="across" lemma="across" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="5" span="1" word="street" lemma="street" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
     <rule type="fa" cat="S[to]\NP">
      <lf start="6" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
      <rule type="ba" cat="S[b]\NP">
       <lf start="7" span="1" word="get" lemma="get" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="8" span="1" word="away" lemma="away" pos="RB" chunk="I-ADVP" entity="O" cat="((S\NP)\(S\NP))/PP" />
        <rule type="fa" cat="PP">
         <lf start="9" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
         <rule type="ba" cat="NP">
          <rule type="fa" cat="NP[nb]">
           <lf start="10" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <rule type="fa" cat="N">
            <lf start="11" span="1" word="red" lemma="red" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <lf start="12" span="1" word="shirted" lemma="shirted" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="13" span="1" word="person" lemma="person" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
          <rule type="lex" cat="NP\NP">
           <rule type="fa" cat="S[ng]\NP">
            <lf start="14" span="1" word="chasing" lemma="chase" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
            <lf start="15" span="1" word="them" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="16" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Two" lemma="two" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="people" lemma="people" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <lf start="2" span="1" word="walk" lemma="walk" pos="VBP" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="3" span="1" word="away" lemma="away" pos="RB" chunk="I-ADVP" entity="O" cat="((S\NP)\(S\NP))/PP" />
     <rule type="fa" cat="PP">
      <lf start="4" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="6" span="1" word="restaurant" lemma="restaurant" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="7" span="1" word="across" lemma="across" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="8" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="9" span="1" word="street" lemma="street" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP">
  <rule type="lex" cat="NP">
   <rule type="fa" cat="N">
    <lf start="0" span="1" word="Two" lemma="two" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
    <lf start="1" span="1" word="adults" lemma="adult" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <rule type="lex" cat="NP\NP">
   <rule type="ba" cat="S[ng]\NP">
    <lf start="2" span="1" word="walking" lemma="walk" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="3" span="1" word="across" lemma="across" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="5" span="1" word="road" lemma="road" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="6" span="1" word="near" lemma="near" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="8" span="1" word="convicted" lemma="convict" pos="VBN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="9" span="1" word="prisoner" lemma="prisoner" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
        <rule type="lex" cat="NP\NP">
         <rule type="fa" cat="S[pss]\NP">
          <lf start="10" span="1" word="dressed" lemma="dress" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
          <rule type="fa" cat="PP">
           <lf start="11" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/(S[adj]\NP)" />
           <lf start="12" span="1" word="red" lemma="red" pos="JJ" chunk="I-NP" entity="O" cat="S[adj]\NP" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>

<ccg>
 <rule type="ba" cat="NP">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="bike" lemma="bike" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="ba" cat="NP\NP">
   <lf start="2" span="1" word="it" lemma="it" pos="PRP" chunk="B-NP" entity="O" cat="NP" />
   <lf start="3" span="1" word="outside" lemma="outside" pos="RB" chunk="I-ADVP" entity="O" cat="(NP\NP)\NP" />
  </rule>
 </rule>
</ccg>

</candc>
